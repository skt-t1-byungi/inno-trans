{
    "name": "inno-trans",
    "description": "simple localization library (inspired by laravel translation)",
    "version": "2.0.0",
    "main": "lib/index.js",
    "unpkg": "dist/inno-trans.min.js",
    "types": "lib/index.d.ts",
    "author": {
        "name": "skt-t1-byungi",
        "email": "tiniwz@gmail.com"
    },
    "keywords": [
        "i18n",
        "translation",
        "localization",
        "localize",
        "globalize"
    ],
    "files": [
        "lib",
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/skt-t1-byungi/inno-trans.git"
    },
    "license": "MIT",
    "scripts": {
        "test": "ava",
        "build": "tsc && bili lib/index.js --format umd-min --no-js --no-map --moduleName InnoTrans",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "ava": "^1.0.0-rc.1",
        "bili": "^3.1.2",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-config-byungi": "^0.0.8",
        "typescript": "^3.1.3"
    },
    "dependencies": {
        "@skt-t1-byungi/array-filter": "^0.1.0",
        "@skt-t1-byungi/array-find": "^0.1.0",
        "@skt-t1-byungi/array-includes": "^0.1.0",
        "@skt-t1-byungi/array-map": "^0.1.0",
        "@skt-t1-byungi/array-reduce": "^0.1.0",
        "tslib": "^1.9.3"
    },
    "ava": {
        "extensions": [
            "ts"
        ],
        "compileEnhancements": false,
        "require": [
            "ts-node/register"
        ]
    }
}
